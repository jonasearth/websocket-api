{
  "name": "onboarding-edge-bff",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "prebuild:prod": "rimraf dist",
    "build": "nest build",
    "build:prod": "nest build --path ./tsconfig.prod.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug=0.0.0.0 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "node --inspect-brk=0.0.0.0 -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --watch --runInBand",
    "test:ci": "jest --ci --testResultsProcessor=\"jest-junit\" --coverage --coverageReporters=cobertura",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk=0.0.0.0 -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "ts-knex": "ts-node -r dotenv/config -r tsconfig-paths/register node_modules/.bin/knex",
    "knex": "node -r dotenv/config node_modules/.bin/knex"
  },
  "dependencies": {
    "@nestjs/axios": "0.0.2",
    "@nestjs/common": "^7.6.18",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.18",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/platform-express": "^7.6.18",
    "@nestjs/platform-socket.io": "^8.0.7",
    "@nestjs/websockets": "^8.1.2",
    "@types/lodash": "^4.14.172",
    "axios": "^0.21.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "common-tags": "^1.8.0",
    "cookie": "^0.4.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "form-data": "^4.0.0",
    "knex": "^0.95.11",
    "lodash": "^4.17.21",
    "nest-winston": "^1.5.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "socket.io": "^4.3.1",
    "socket.io-client": "^4.3.2",
    "winston": "^3.3.3",
    "ws": "^8.2.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.18",
    "@types/chai": "^4.2.19",
    "@types/common-tags": "^1.8.0",
    "@types/express": "^4.17.12",
    "@types/faker": "^5.5.8",
    "@types/jest": "^26.0.23",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.17.9",
    "@types/sinon": "^10.0.2",
    "@types/supertest": "^2.0.11",
    "@types/ws": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-airbnb-typescript-prettier": "^4.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "faker": "^5.5.3",
    "jest": "^27.0.6",
    "jest-junit": "^12.2.0",
    "prettier": "^2.3.2",
    "sinon": "^11.1.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "testPathIgnorePatterns": [
      ".*\\e2e\\.spec\\.ts$"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "./src/**/*.(t|j)s"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/database/",
      "/config/",
      ".module.ts",
      "<rootDir>/src/main.ts",
      "/swagger/",
      ".env.ts",
      ".enum.ts",
      "test/",
      "/scripts/"
    ],
    "testEnvironment": "node",
    "testResultsProcessor": "jest-junit"
  }
}
